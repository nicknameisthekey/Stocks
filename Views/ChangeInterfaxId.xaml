<Window x:Class="Stocks.Views.ChangeInterfaxId"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stocks.Views"
        mc:Ignorable="d" Height="150" Width="200">
    <StackPanel>
        <TextBlock HorizontalAlignment="Center" FontSize="18">Новый ID:</TextBlock>
        <TextBox x:Name="IdTextbox" Width="124" Height="30" FontSize="18" Margin="10"></TextBox>
        <Button HorizontalAlignment="Center" Height="40" Width="90"
                Click="SaveBtn_Click">Сохранить</Button>
    </StackPanel>
</Window>
